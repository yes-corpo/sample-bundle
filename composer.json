{
    "name": "yes-corpo/sample-bundle",
    "description": "Sample bundle for YesCorpo",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1.0",
        "symfony/framework-bundle": "6.2.*",
        "symfony/console": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "YesCorpo\\Bundle\\SampleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YesCorpo\\Bundle\\SampleBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/test-pack": "^1.1"
    }
}
